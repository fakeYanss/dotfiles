{
  "coc.source.around.enable": true,
  "coc.source.buffer.enable": true,
  "coc.source.file.enable": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "html",
    "yaml",
    "json",
    "python"
  ],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "snippets.ultisnips.directories": [
    "~/.config/nvim/snips"
  ],
  "css.enable": true,
  "css.trace.server": "verbose",
  "html.enable": true,
  "html.trace.server": "messages",
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "phtml"
  ],
  "html.format.enable": true,
  "html.format.unformatted": null,
  "html.format.contentUnformatted": "pre,code,textarea",
  "html.format.indentInnerHtml": true,
  "html.suggest.html5": true,
  "html.autoClosingTags": true,
  "json.enable": true,
  "json.trace.server": "messages",
  "json.format.enable": true,
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home",
  "java.trace.server": "verbose",
  "java.format.enabled": true,
  "java.import.maven.enabled": true,
  "java.maven.downloadSources": true,
  "java.import.gradle.version": "6.9.1",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home"
    },
    {
      "name": "JavaSE-1.8",
      "path": "/Library/Java/JavaVirtualMachines/adoptopenjdk-8.jdk/Contents/Home",
      "default": true
    }
  ],
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  }
}
